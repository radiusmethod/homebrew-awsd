{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^Formula/awsd.rb$"],
      "matchStrings": [
        "ver = \"(?<currentValue>.*)\""
      ],
      "depNameTemplate": "pjaudiomv/awsd",
      "datasourceTemplate": "github-releases"
    }
  ]
}
